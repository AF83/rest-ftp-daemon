defaults: &defaults
  daemonize: 1
  port: 3200
  workers: 1
  adminpwd: "admin"
  pidfile: /tmp/rftpd.pid

  transfer:
    chunk_size: 100000
    notify_size: 10000000

  conchita:
    timer: 5
    clean_failed: 3600
    clean_finished: 600

  logs:
    thin:
    api:
    workers:

production:
  <<: *defaults
